[alias]
  co = checkout
  s = status -sb
  g = grep --break --heading --line-number
  lg = log --decorate --graph
  branches = for-each-ref --sort=committerdate --format=\"%(color:green)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\" refs/heads
[fetch]
  prune = true
